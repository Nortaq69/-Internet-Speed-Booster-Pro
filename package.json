{
  "name": "internet-speed-booster",
  "version": "1.0.0",
  "description": "Advanced Internet Speed Booster with multiple optimization techniques",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dev": "electron . --dev"
  },
  "keywords": [
    "internet",
    "speed",
    "booster",
    "optimization",
    "network"
  ],
  "author": "NexusHub",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "node-powershell": "^5.0.1",
    "ping": "^0.4.4",
    "regedit": "^3.0.3",
    "speedtest-net": "^1.6.2",
    "systeminformation": "^5.21.7"
  },
  "build": {
    "appId": "com.nexushub.internetspeedbooster",
    "productName": "Internet Speed Booster Pro",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
